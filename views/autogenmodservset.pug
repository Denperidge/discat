style
    include autogenmodservset.css
script(src="autogenmodservset.js")
- var keys = Object.keys(settings);
- for (var i=0; i < keys.length; i++)
    case typeof settings[keys[i]]
        when "number"
            input(type="number" placeholder=keys[i] value=settings[keys[i]])
        when "string"
            input(type="text" placeholder=keys[i] value=settings[keys[i]])
        when "boolean"
            if settings[keys[i]] == true
                input(type="checkbox" checked)
                span #{keys[i]}
            else
                input(type="checkbox")
                span #{keys[i]}
div#buttons
    button#btnSave(onclick="saveAutoGeneratedModuleSettings();") Save
    button#btnCancel(onclick="document.getElementById(\"overlay\").click();") Cancel